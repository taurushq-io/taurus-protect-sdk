{
  "name": "@taurushq/protect-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Taurus-PROTECT API - cryptocurrency custody and transaction management",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "generate": "./scripts/generate-openapi.sh && ./scripts/generate-proto.sh",
    "generate:openapi": "./scripts/generate-openapi.sh",
    "generate:proto": "./scripts/generate-proto.sh",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "taurus",
    "protect",
    "cryptocurrency",
    "custody",
    "blockchain",
    "wallet",
    "api",
    "sdk"
  ],
  "author": "Taurus SA",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taurushq-io/taurus-protect-sdk"
  },
  "bugs": {
    "url": "https://github.com/taurushq-io/taurus-protect-sdk/issues"
  },
  "homepage": "https://github.com/taurushq-io/taurus-protect-sdk/tree/main/taurus-protect-sdk-typescript#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "long": "^5.2.3",
    "protobufjs": "^7.2.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "ts-proto": "^1.174.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
