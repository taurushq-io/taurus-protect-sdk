{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2022", "DOM"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "useUnknownInCatchVariables": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": false,
    "noImplicitOverride": false,
    "noPropertyAccessFromIndexSignature": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "dist",
    "tests",
    "src/internal/proto/action*.ts",
    "src/internal/proto/airgap*.ts",
    "src/internal/proto/annotations.ts",
    "src/internal/proto/any.ts",
    "src/internal/proto/api_request_to_validate.ts",
    "src/internal/proto/apikey.ts",
    "src/internal/proto/approvers.ts",
    "src/internal/proto/asset*.ts",
    "src/internal/proto/audit*.ts",
    "src/internal/proto/authentication*.ts",
    "src/internal/proto/blockchain*.ts",
    "src/internal/proto/change*.ts",
    "src/internal/proto/code.ts",
    "src/internal/proto/contract-template*.ts",
    "src/internal/proto/credentials.ts",
    "src/internal/proto/currency.ts",
    "src/internal/proto/cursor.ts",
    "src/internal/proto/descriptor.ts",
    "src/internal/proto/duration.ts",
    "src/internal/proto/empty.ts",
    "src/internal/proto/error_details.ts",
    "src/internal/proto/errors.ts",
    "src/internal/proto/exchange*.ts",
    "src/internal/proto/fee*.ts",
    "src/internal/proto/fiat*.ts",
    "src/internal/proto/health*.ts",
    "src/internal/proto/http*.ts",
    "src/internal/proto/index.ts",
    "src/internal/proto/job*.ts",
    "src/internal/proto/kv.ts",
    "src/internal/proto/maintenance*.ts",
    "src/internal/proto/metadata.ts",
    "src/internal/proto/multi-factor*.ts",
    "src/internal/proto/openapiv2.ts",
    "src/internal/proto/price*.ts",
    "src/internal/proto/report*.ts",
    "src/internal/proto/request*.ts",
    "src/internal/proto/reservation.ts",
    "src/internal/proto/rule*.ts",
    "src/internal/proto/scim*.ts",
    "src/internal/proto/score*.ts",
    "src/internal/proto/sso*.ts",
    "src/internal/proto/staking*.ts",
    "src/internal/proto/statistics*.ts",
    "src/internal/proto/status*.ts",
    "src/internal/proto/steward*.ts",
    "src/internal/proto/struct.ts",
    "src/internal/proto/tag*.ts",
    "src/internal/proto/taurus-network*.ts",
    "src/internal/proto/timestamp.ts",
    "src/internal/proto/token-metadata*.ts",
    "src/internal/proto/transaction*.ts",
    "src/internal/proto/user*.ts",
    "src/internal/proto/validator.ts",
    "src/internal/proto/visibility.ts",
    "src/internal/proto/wallet*.ts",
    "src/internal/proto/webhook*.ts",
    "src/internal/proto/whitelist*.ts",
    "src/internal/proto/workflow*.ts",
    "src/internal/proto/wrappers.ts",
    "src/internal/proto/xtzbaker*.ts"
  ]
}
